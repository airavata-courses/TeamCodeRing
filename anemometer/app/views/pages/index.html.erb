

<% if current_user %>
  <div class="page-header">
    <h1>Welcome back!</h1> <br>
  </div>
  

  <div class="well well-lg">
    <%= render partial: "dataingestor"  %>
  </div>

<% else %>
  <div class="jumbotron">
    <div class="container">
      <h1>Welcome!</h1>
      <p>Sign up to get started.</p>
      <p>
        <%= link_to new_user_path, class: 'btn btn-primary btn-lg' do %>
          Sign Up &raquo;
        <% end %>
        <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in",class: 'btn btn-primary btn-lg' %>
      </p>
    </div>
  </div>
 <% end %>

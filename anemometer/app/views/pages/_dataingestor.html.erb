  <h2> Select Station </h2>
  <form action="#">
    <div class="form-group row">
      <label class="col-xs-2" for="station">NEXRAD STATION</label>
      <div class="col-xs-10">
        <%= select_tag(:stations, options_for_select(@station_list), class: "col-xs-5") %>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-xs-2" for="time">TIME & DATE</label>
      <div class="col-xs-10">
        <%= text_field :start, :date, class: 'datetimepicker' %>
      </div>
    </div>
    <input type="submit" value="Submit">
  </form>
</div>



